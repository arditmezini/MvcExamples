@model MvcExamples.Models.Visitor.VisitorDto

<br />
<div class="container">
    <div class="panel panel-default">

        <div class="panel-heading">
            <h3 class="panel-title">Visitor Details</h3>
        </div>

        <div class="panel-body">
            <div class="row">
                <div class="col-md-3">
                    @Html.LabelFor(model => model.FirstName)
                </div>
                <div class="col-md-3">
                    @Html.DisplayFor(model => model.FirstName)
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    @Html.LabelFor(model => model.LastName)
                </div>
                <div class="col-md-3">
                    @Html.DisplayFor(model => model.LastName)
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    @Html.LabelFor(model => model.Email)
                </div>
                <div class="col-md-3">
                    @Html.DisplayFor(model => model.Email)
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    @Html.LabelFor(model => model.Gender)
                </div>
                <div class="col-md-3">
                    @Html.DisplayFor(model => model.Gender)
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    @Html.LabelFor(model => model.IpAddress)
                </div>
                <div class="col-md-3">
                    @Html.DisplayFor(model => model.IpAddress)
                </div>
            </div>
        </div>

        <div class="panel-footer">
            @Html.ActionLink("Back to List", "Index", "Visitor", new { @class = "btn btn-info" })
        </div>

    </div>
</div>

